---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
    <channel>
        <title>{{ site.name }}</title>
        <description></description>
        <link>{{ site.url }}</link>
        {% for post in site.posts %}
            <item>
                <title>{{ post.title }}</title>
                <description>{{ post.content | xml_escape }}</description>
                <published>{{ post.date }}</published>
                <link>{{ site.url }}{{ post.url }}</link>
            </item>
        {% endfor %}
    </channel>
</rss>
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>animalcoders.com</title>
    <link>http://animalcoders.com/</link>
    <atom:link href="http://animalcoders.com/feed.xml" rel="self" type="application/rss+xml" />
    <description>Animal coders feed</description>
    <language>es</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://animalcoders.com{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>Animal Coders</author>
      <guid>http://animalcoders.com{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel> 
</rss>


